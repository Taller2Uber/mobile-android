<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.slazzari.taller2uber.activity.Register.RegisterDriverActivity">

    <Button
        android:id="@+id/register_driver_add_car_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Agregar auto"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="228dp" />

    <Button
        android:id="@+id/register_driver_confirm_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Continuar"

        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintRight_toLeftOf="parent"

        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="447dp" />
</android.support.constraint.ConstraintLayout>
